<style>
  body {
    font-family: <%=(is_font_family && is_font_family !=="") ? is_font_family.replace('-', ' ') + ' !important': ''%>;
  }

  .section-init .el-button {
    color: <%=button_style && button_style.fontColor ? button_style.fontColor: ''%>;
    background: <%=button_style && button_style.background ? button_style.background: ''%>;
  }


  .section-init.hardshadow .el-button {
    box-shadow: 4px 4px 0 0 <%=button_style && button_style.background ? button_style.background + '66': '#3346A266'%>;
  }

  .section-init.softshadow .el-button {
    box-shadow: 2px 2px 0 0 <%=button_style && button_style.background ? button_style.background + '66': '#3346A266'%>;
  }

  .remove-margin p {
    margin-bottom: 0 !important;
  }
</style>